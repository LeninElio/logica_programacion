Algoritmo ejercicio_oa
	Imprimir 'Ingrese el primer valor: '
	Leer valor_1
	Imprimir 'Ingrese el segundo valor: '
	Leer valor_2
	
	Imprimir 'La suma es: ', valor_1 + valor_2
	Imprimir 'La resta es: ', valor_1 - valor_2
	Imprimir 'La multiplicacion es: ', valor_1 * valor_2
	Imprimir 'La division es: ', valor_1 / valor_2
	Imprimir 'La modulo es: ', valor_1 % valor_2
	Imprimir 'La potencia es: ', valor_1 ^ valor_2
